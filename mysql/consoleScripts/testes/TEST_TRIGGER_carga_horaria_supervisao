
-- teste - Regra de negócio 	A soma das cargas horárias em que o 
-- funcionário supervisiona e trabalha em   projetos não pode ultrapassar a carga horária do funcionário

-- aqui veremos um erro
INSERT INTO Supervisor(ID_Funcionario, ID_Projeto, Carga_Horaria_Supervisao)
VALUES (1, 2, 119);


-- aqui o insert passa ja que esta menor que a Carga_Horaria de Funcionario_Projeto
INSERT INTO Supervisor(ID_Funcionario, ID_Projeto, Carga_Horaria_Supervisao)
VALUES (1, 2, 9);



